MODULE mainTest;

IMPORT Out,
   mT1 := collectionsTest;

VAR
   error:INTEGER;

BEGIN
   error := 0;
   Out.String("Начало тестов");Out.Ln();
   error := error + mT1.error;
   IF error # 0 THEN
      Out.String("Тесты провалены. Всего ошибок: "); Out.Int(error, 2); Out.Ln()
   ELSE
      Out.String("Тесты пройдены. Ошибок 0");Out.Ln()
   END;
   Out.String("Конец тестов");Out.Ln();
END mainTest.